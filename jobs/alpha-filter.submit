#!/bin/sh

SCRIPT_NAME="alpha-filter"
COMP_DICT_DIR="comp-dicts"
ALPHA="bonferroni"
IDX=$1
SECTION_SIZE=5
JOB_NAME=${SCRIPT_NAME}-${IDX}-${ALPHA}

sbatch -J $JOB_NAME \
    --time=00-01:00:00 \
    --nodes=1 \
    --ntasks=1 \
    --mem=128G \
    -o slurm-output/${JOB_NAME}.out \
    -e slurm-output/${JOB_NAME}.err \
    jobs/${SCRIPT_NAME}.sh $COMP_DICT_DIR $ALPHA $IDX $SECTION_SIZE
